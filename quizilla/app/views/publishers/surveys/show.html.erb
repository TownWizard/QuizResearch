<div id="survey">
  
  <div id="survey-name">
    <table>
      <tr>
        <td>Reward Interstitial of your new survey:</td>
        <td><%= @survey.reward_interstitial %></td>
      </tr><tr>
        <td>Reward URL:</td>
        <td><%= @survey.reward_url %></td>
      </tr><tr>
        <td>Quiz:</td>
        <td><%= @survey.quiz.name %></td>
      </tr><tr>
        <td colspan="2"><%= link_to_remote( "Edit Survey", :url => edit_publishers_survey_path( :survey_id => @survey.id ), :method => 'GET', :update => "survey-form-container" ) %></td>
      </tr>
    </table>
    <div id="survey-form-container"></div>
  </div>
</div>
